{
    "name": "evitenic/filament-comments",
    "description": "Add comments to your Filament Resources.",
    "keywords": [
        "laravel",
        "filament-comments"
    ],
    "homepage": "https://github.com/evitenic/filament-comments",
    "support": {
        "issues": "https://github.com/evitenic/filament-comments/issues",
        "source": "https://github.com/evitenic/filament-comments"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Parallax",
            "email": "luke@parall.ax",
            "role": "Developer"
        },
         {
            "name": "Evitenic",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0 | ^4.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0 | ^11.0 | ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Evitenic\\FilamentComments\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Evitenic\\FilamentComments\\FilamentCommentsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
